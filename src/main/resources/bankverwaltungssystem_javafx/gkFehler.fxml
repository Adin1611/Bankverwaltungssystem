<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bankverwaltungssystem.bankverwaltungssystem_javafx.GkFehler">
   <children>
      <Label fx:id="lblGKFehler" layoutX="75.0" layoutY="10.0" prefHeight="50.0" prefWidth="250.0" text="Girokonto-Fehler">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label fx:id="lblGrund" layoutX="37.5" layoutY="70.0" prefWidth="325.0" text="Das Girokonto konnte nicht erstellt werden, da das Girokonto&#10; bzw. die Kontonummer des Girokontos schon vorhanden ist." wrapText="true" />
      <Button fx:id="btnNeuesGK" layoutX="130.0" layoutY="120.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="140.0" style="-fx-background-color: A8A8A8;" text="Neues Girokonto&#10;      erstellen">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
